# React.js Gitignore

# Logs
logs
*.log
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# Runtime data
pids
*.pid
*.seed
*.pid.lock

# Directory for instrumented libs generated by jscoverage/Jest
coverage/

# Dependency directories
node_modules/
bower_components/

# Build output
dist/
build/
out/
.temp/
.next/
.cache/
.nyc_output/

# Production environment files
.env
.env.*.local
.env.local
.env.development.local
.env.test.local
.env.production.local

# IDEs and editors
.vscode/
.idea/
*.suo
*.ntvs*
*.njsproj
*.sln
*.sw?

# OS generated files
.DS_Store
Thumbs.db

# Optional npm cache directory
.npm/

# Optional eslint cache
.eslintcache

# Optional stylelint cache
.stylelintcache

# Optional Prettier cache
.prettierignore
.prettiercache

# Parcel cache
.cache/
.parcel-cache/

# Storybook build
storybook-static/

# Testing
jest-test-results.json
coverage-final.json

# TypeScript
*.tsbuildinfo

# Miscellaneous
*.tgz
*.log
yarn.lock
package-lock.json

# Firebase or other hosting platforms
.firebaserc
.firebase/
functions/node_modules/

# VSCode extensions like settings or launch configs
.vscode/settings.json
.vscode/launch.json
.vscode/tasks.json

# Local development SSL certs
*.pem

# Mac system files
.AppleDouble
.LSOverride
Icon?
._*

# Snapcraft
.snapcraft/

# Netlify
.netlify/

# Next.js
.next/

# Temporary file storage
tmp/
temp/
*.tmp
*.bak

# Yarn Plug'n'Play
.pnp*
.yarn/cache
.yarn/unplugged
.yarn/build-state.yml
.yarn/install-state.gz

# Redux devtools extension files (if persisted)
redux-devtools-extension/
